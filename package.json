{
  "name": "p5ble",
  "version": "0.0.1",
  "description": "A javascript library that enables communication between BLE devices and a p5 sketch.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *test.js src",
    "build:umd": "webpack --output-filename index.umd.js --mode development",
    "build:umd.min": "webpack --output-filename index.umd.min.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yining1023/p5.ble.js.git"
  },
  "keywords": [
    "p5js",
    "ble"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Yining Shi <yining@nyu.edu> (https://1023.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yining1023/p5.ble.js/issues"
  },
  "homepage": "https://github.com/yining1023/p5.ble.js#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "json-loader": "0.5.7",
    "rimraf": "2.6.2",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
